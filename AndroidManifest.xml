<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.com.example.mydemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mydemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mydemo.SplashAndengine"></activity>
        <activity android:name="com.example.mydemo.HomeActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
    </application>

</manifest>
